{
  "expo": {
    "name": "Seniar Chat",
    "slug": "seniar-chat",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "seniar-chat",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFEF7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seniar.chat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFEF7"
      },
      "package": "com.seniar.chat"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
